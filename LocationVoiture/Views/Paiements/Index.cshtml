@model IEnumerable<LocationVoiture.Models.Paiement>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.libele)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.libele)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_paiement }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_paiement }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_paiement })
        </td>
    </tr>
}

</table>
