

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="wrapper d-flex">
    <div class="sideMenu bg-mattBlackLight">
        <div class="sidebar">
            <ul class="navbar-nav mt-3">
                <li class="nav-item">
                    <a href="~/AdminDashboard" class="nav-link px-2">
                        <i class="material-icons icon">
                            dashboard
                        </i>
                        <span class="text">@EcommerceApp.Resources.Views.Shared._OwnerDashboard.dashboard</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/ApplicationUsers/Index" class="nav-link px-2">
                        <i class="material-icons icon">
                            person
                        </i>
                        <span class="text">@EcommerceApp.Resources.most.users</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/FavoriteLists/Index" class="nav-link px-2">
                        <i class="material-icons icon">
                            favorite
                        </i>
                        <span class="text">@EcommerceApp.Resources.Views.Shared.Dashboard.favorite_list</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/BlackLists/Index" class="nav-link px-2">
                        <i class="material-icons icon">
                            thumb_down
                        </i>
                        <span class="text">@EcommerceApp.Resources.Views.Shared.Dashboard.black_list</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/Products/Index" class="nav-link px-2">
                        <i class="material-icons icon">
                            bookmark_border
                        </i><span class="text">@EcommerceApp.Resources.Views.Shared._OwnerDashboard.product</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/Reservations/Index" class="nav-link px-2">
                        <i class="material-icons icon">
                            view_module
                        </i>
                        <span class="text">@EcommerceApp.Resources.Views.Shared._TenantDashboard.reservation</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/Offres/index" class="nav-link px-2">
                        <i class="material-icons icon">
                            %
                        </i>
                        <span class="text">Offer</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/Categories" class="nav-link px-2">
                        <i class="material-icons icon">
                            library_books
                        </i>
                        <span class="text">@EcommerceApp.Resources.Views.Shared._OwnerDashboard.model</span>
                    </a>
                </li>

                <li class="nav-item">
                    <a href="~/Account/Edit" class="nav-link px-2">
                        <i class="material-icons icon">
                            account_box
                        </i>
                        <span class="text">@EcommerceApp.Resources.Views.Shared._OwnerDashboard.profil</span>
                    </a>
                </li>

            </ul>
        </div>
    </div>
    <div class="content">
        <main>
            <div class="container-fluid">
                @RenderBody()
            </div>
        </main>
    </div>
</div>

@section chartSection{
    @RenderSection("chart", required: false)
}
